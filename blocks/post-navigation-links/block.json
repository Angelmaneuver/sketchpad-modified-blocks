{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "sketchpad-modified-blocks/post-navigation-links",
	"version": "0.1.0",
	"title": "Post navigation links",
	"category": "theme",
	"description": "Displays the next and previous post link that is adjacent to the current post.",
	"textdomain": "sketchpad-modified-blocks",
	"keywords": [ "Post navigation links" ],
	"styles": [],
	"usesContext": [ "postId" ],
	"supports": {
		"align": true,
		"reusable": false,
		"html": false,
		"color": {
			"gradients": true,
			"link": true,
			"__experimentalDefaultControls": {
				"background": true,
				"text": true
			}
		},
		"spacing": {
			"margin": [ "top", "bottom" ],
			"padding": true
		},
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true,
			"__experimentalDefaultControls": {
				"color": true,
				"radius": true,
				"style": true,
				"width": true
			}
		}
	},
	"attributes": {},
	"example": {},
	"editorScript": "file:post-navigation-links.js",
	"editorStyle": "file:post-navigation-links.css",
	"style": "file:style-post-navigation-links.css"
}
